{
    "transform": {
        "^.+\\.(ts?|tsx?|js?|jsx?)$": "babel-jest"
    },
    "transformIgnorePatterns": ["<rootDir>/node_modules/"],
    "moduleFileExtensions": [
        "js"
    ],
    "testMatch": [
        "<rootDir>/(tests/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx))"
    ]
}